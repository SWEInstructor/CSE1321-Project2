  PUBLIC static String inv[] = {"","","","",""};
  //Adds the item to inventory
  PUBLIC void addItemToInv(String name)

BEGIN
  FOR i = 0 TO length of inv
   BEGIN
    IF(inv[i] == "")
    BEGIN
      inv[i] = name;
      
      Print(name + " has been added to " + NAME + " \'s inventory");
    END IF
   END FOR
END adItemToInv
    
    //Prints the inventory
  PUBLIC void checkInv()
   BEGIN 
    FOR(String x: inv)
    BEGIN 
      PRINT(x + ", ");
     END FOR
    END checkInv
   
   //checks to see if player has a certain item
   PUBLIC boolean hasItem(String itemName)
   BEGIN
    FOR i=0 TO length of inv
    BEGIN
      IF(itemName == inv[i])
      BEGIN
        return true;
      END IF
    END FOR
    return false;
   END hasItem
