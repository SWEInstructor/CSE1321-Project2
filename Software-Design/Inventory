  PUBLIC static String inv[] = {"","","","",""};
  //Adds the item to inventory
  PUBLIC void addItemToInv(String name)

BEGIN
  FOR i = 0 TO length of inv
   BEGIN
    IF(inv[i] == "")
    BEGIN
      inv[i] = name;
      
      Print("You have added:" + name + ",");
      return;
    END
   END
END
    
    //Prints the inventory
  PUBLIC void printInv()
   BEGIN
    FOR(String x: inv)
    BEGIN
      PRINT(x);
     END
    END
   
