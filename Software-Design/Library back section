IF(place = "Library back section" )BEGIN
  PRINTLN(NAME + " made it to the back of the Library");
  PRINTLN("What does" + NAME + "wanna do?!");

  command = checkInput(inputs);
IF(command == "visit") THEN
    place = visit(place);
END IF

IF(command == "help”) THEN 
    help();
END  IF

IF(command == "check inventory")THEN
    checkInv();
  END IF
  
IF(command == "look") THEN
    PRINTLN(NAME + "looks around and spots more books (its like you're in library or something), a chair, and a map on a table.");
END ELSE IF

IF(command == "use") THEN
   PRINTLN("What is " + NAME + "going to use" );
   CREATE String[] librarybackObjects;
    IF(!(hasItem("map"))) THEN
   librarybackObjects = {"chair", "map" , "books" }
    command = checkInput(librarybackObjects);
    END ELSE IF
    
     ELSE IF(hasItem("map")) THEN
     librarybackObjects = {"chair", "books"}
     command = checkInput(librarybackObjects);
     END ELSE IF
 
IF(command == "chair") THEN
   PRINTLN(NAME + "sits down in the chair....lame");
   
END ELSE IF

IF(command == "books")THEN
   PRINTLN(NAME + "opens a book, HOW EXCITING! (◔_◔)");
   
END ELSE IF

IF(command == "map") THEN
      IF(!(hasItem("real map"))) THEN 
        
	 addItemToInv("real map");
   
   END ELSE IF
   
   
   
   
END
