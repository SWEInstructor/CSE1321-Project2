BEGIN
help();
IF(command=="look")
    PRINTLN ("You see a dresser in the corner because you have eyes. You probably could snoop around in it since you have hands too.");
    PRINTLN ("You also spot a plant like the one at your house, only this one is actually alive.");
    
    END
 IF(command=="use")
    CREATE String[] friendsHouseObjects;
    friendsHouseObjects ={"dresser", "plant", "door"}
    CREATE String ineractWith
    PRINTLN (What are you going to use: ?");
    READ interactWith
    
    IF(interactWith== "dresser")
    PRINTLN ("Wow, you're nosy. You walk over to his dresser and try to open a drawer.");
    PRINTLN ("The drawer won't open because the game developers are poor college kids who don't have enough time to code something inside.");
    END IF
 
    IF(interactWith== "plant")
    PRINTLN ("At first you were impressed your friend could keep a plant alive longer than you did, but when you pick it you realize it's fake.");
    END IF
    END IF
  
  IF (command == "check inventory") THEN
    checkInv();
    END IF
    
